95 PRINT "Enter a number to find its factors!"
97 PRINT
100 INPUT A
110 PRINT
120 LET F=0
140 IF A/2=INT(A/2) THEN LET A=A/2: PRINT " 2 x";: LET F=1: GOTO 140
145 LET I=3
150 LET E=INT(SQR(A)) + 2
155 LET F=0
160 FOR N=I TO E STEP 2
180 IF A/N=INT(A/N) THEN PRINT N;"x";:A=A/N:I=N:N=E:F=1
200 NEXT N
210 REM PRINT A;" "; N;" "; I;" "; E;" ";F
220 IF A>N AND F<>0 THEN GOTO 155
230 PRINT A
235 PRINT
240 GOTO 100
